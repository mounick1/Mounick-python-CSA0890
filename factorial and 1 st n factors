num=int(input("enter the numbers:"))
y=[]
for i in range(1,num+1):
    if num%i==0:
        y.append(i)
print(y)
print("the factors are:",len(y))
n=int(input("enter the number:"))
if n>len(y):
    print("invalid")
else:
    print("first",n,"factors are:")
    for k in range (0,n):
        print(y[k],end=" ")

num= int(input("Enter the start of the range: "))
if num < 2:
    print("is not a prime")
else:
    is_prime = True
    for i in range(2, int(num ** 0.5) + 1):
        if num % i == 0:
            is_prime = False
            break
    if  is_prime:
        print("prime number")
    else:
        print("composite number")
